enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ELDAW-8
gpgcheck=1

[eldaw-source]
name=Enterprise Linux Digital Audio Workstation $releasever - $basearch - Source
#baseurl=https://download.teknik.tv/pub/eldaw/$releasever/Everything/SRPMS
metalink=https://mirrors.teknik.tv/metalink?repo=eldaw-source-$releasever&arch=$basearch&infra=$infra&content=$contentdir
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ELDAW-8
