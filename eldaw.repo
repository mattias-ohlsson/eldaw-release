[eldaw]
name=Enterprise Linux Digital Audio Workstation $releasever - $basearch
#baseurl=https://download.teknik.tv/pub/eldaw/$releasever/Everything/$basearch
metalink=https://mirrors.teknik.tv/metalink?repo=eldaw-$releasever&arch=$basearch&infra=$infra&content=$contentdir
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ELDAW-8

[eldaw-debuginfo]
name=Enterprise Linux Digital Audio Workstation $releasever - $basearch - Debug
#baseurl=https://download.teknik.tv/pub/eldaw/$releasever/Everything/$basearch/debug
