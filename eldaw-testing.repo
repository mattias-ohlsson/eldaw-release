[eldaw-testing]
name=Enterprise Linux Digital Audio Workstation $releasever - Testing - $basearch
#baseurl=https://download.teknik.tv/pub/eldaw/testing/$releasever/Everything/$basearch
metalink=https://mirrors.teknik.tv/metalink?repo=testing-eldaw-$releasever&arch=$basearch&infra=$infra&content=$contentdir
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-ELDAW-8
skip_if_unavailable=True

